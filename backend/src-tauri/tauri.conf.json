{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "AI-Brain",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../build/web",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run devflutter run -d web-server --web-port 3000",
    "beforeBuildCommand": "npm run buildflutter build web"
  },
  "app": {
    "windows": [
      {
        "title": "AI-Brain",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
